{
  "type": "discord",
  "display": "discord.js (Discord)",
  "data": {
    "bot-js-file": {
      "type": "string",
      "desc": "The main bot file.",
      "display": "Main bot file",
      "required": true,
      "value": "bot.js"
    }
  },
  "install": [
    {
      "type": "command",
      "commands": [
        "npm i"
      ]
    }
  ],
  "run": {
    "command": "node ./${bot-js-file}",
    "stop": "stop"
  },
  "environment": {
    "type": "docker",
    "image": "node"
  }
}