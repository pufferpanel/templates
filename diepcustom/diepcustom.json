{
  "display": "Diep Custom",
  "type": "node",
  "install": [
    {
      "commands": [
        "git clone https://github.com/ABCxFF/diepcustom.git .",
        "npm install yarn",
        "yarn install"
      ],
      "type": "command"
    },
    {
      "file": "./src/config.ts",
      "replace": "export const defaultAccessLevel: AccessLevel = AccessLevel.PublicAccess;",
      "search": "export const defaultAccessLevel: AccessLevel = AccessLevel.BetaAccess;",
      "type": "alterfile"
    }
  ],
  "run": {
    "stopCode": 2,
    "command": "yarn run server",
    "workingDirectory": "",
    "pre": [],
    "post": [],
    "environmentVars": {
      "DEV_PASSWORD_HASH": "${DEV_PASSWORD_HASH}",
      "PORT": "${PORT}"
    }
  },
  "data": {
    "DEV_PASSWORD_HASH": {
      "type": "string",
      "display": "Devpass (SHA-256)"
    },
    "PORT": {
      "type": "string",
      "display": "Port",
      "required": true,
      "value": "8080"
    }
  },
  "environment": {
    "type": "standard"
  },
  "requirements": {
    "binaries": [
      "node",
      "npm"
    ]
  }
}
